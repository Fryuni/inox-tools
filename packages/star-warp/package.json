{
  "name": "@inox-tools/star-warp",
  "version": "0.2.0",
  "description": "Warp-drive search for Astro sites using Pagefind",
  "keywords": [
    "astro-integration",
    "withastro",
    "starlight",
    "astro",
    "utils"
  ],
  "license": "MIT",
  "author": "Luiz Ferraz <luiz@lferraz.com>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./routes/*": "./routes/*"
  },
  "files": [
    "dist",
    "src",
    "routes",
    "virtual.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublish": "pnpm run build",
    "test": "echo 'No tests'",
    "test:dev": "vitest --coverage.enabled=true"
  },
  "dependencies": {
    "@inox-tools/utils": "workspace:^",
    "astro-integration-kit": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "astro": "^5.1.1",
    "jest-extended": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vite": "^6.0.6",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "@astrojs/starlight": "catalog:lax",
    "astro": "catalog:lax"
  }
}
