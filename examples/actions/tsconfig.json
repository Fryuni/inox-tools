{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "target": "ES2020",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noUnusedLocals": false,
    "strictPropertyInitialization": false,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "exactOptionalPropertyTypes": false
  }
}
