---
import { state } from '../state.js';

const injectedState = 'bar';

state.set(injectedState);
export const partial = true
---

<pre id="injected-state-bar"></pre>
<script>
	import { serializedState } from '../state.js';

	serializedState.subscribe((value) => {
		document.getElementById('injected-state-bar')!.innerHTML = value;
	});
</script>
