---
import { getCollection, type CollectionEntry } from 'astro:content';

export const prerender = true;
export const partial = true;

const entries: CollectionEntry<'on-demand'>[] = await getCollection('on-demand');
---

{
	entries.map((entry) => (
		<p>
			{entry.id}: {entry.data.content}
		</p>
	))
}
