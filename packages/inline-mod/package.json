{
  "name": "@inox-tools/inline-mod",
  "version": "0.0.0",
  "description": "Define a virtual module inline with any reference to buildtime values",
  "repository": "https://github.com/Fryuni/inox-tools.git",
  "license": "MIT",
  "author": "Luiz Ferraz <luiz@lferraz.com>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "README.md",
    "dist"
  ],
  "type": "module",
  "keywords": [
    "vite-plugin"
  ],
  "devDependencies": {
    "@vitest/ui": "^1.1.3",
    "typescript": "^4.6.4",
    "vite": "^3.2.0",
    "vitest": "^1.1.3"
  },
  "scripts": {
    "dev": "vite --host",
    "clean": "rimraf ./lib",
    "build": "tsup ./src/index.ts --outDir 'lib' --dts --format cjs,esm",
    "prepublish": "npm run clean && npm run build"
  }
}
