{
  "name": "@inox-tools/inline-mod",
  "version": "1.3.1",
  "description": "Define a virtual module inline with any reference to buildtime values",
  "keywords": [
    "vite-plugin"
  ],
  "repository": "https://github.com/Fryuni/inox-tools.git",
  "license": "(MIT AND Apache-2.0)",
  "author": "Luiz Ferraz <luiz@lferraz.com>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "default": "./dist/vite.js"
    }
  },
  "files": [
    "README.md",
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublish": "pnpm run build",
    "test": "vitest run --coverage",
    "test:dev": "vitest --coverage.enabled=true"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/debug": "^4.1.12",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "jest-extended": "^4.0.2",
    "tsup": "^8.0.2",
    "upath": "^2.0.1",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "vite": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  }
}
